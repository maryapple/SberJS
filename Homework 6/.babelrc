{
    "presets": [[
        "@babel/preset-env",
        {
            "modules": "auto",
            "targets": {
                "browsers": [
                    "last 2 versions",
                    "> 1%",
                    "not ie < 11"
                ]
            }
        }
    ]],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ]
}
